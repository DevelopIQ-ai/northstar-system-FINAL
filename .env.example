# Microsoft Graph API Configuration (for Outlook email sending)
MS_CLIENT_ID=your-microsoft-app-client-id
MS_CLIENT_SECRET=your-microsoft-app-client-secret
ENCRYPTED_REFRESH_TOKEN=iv:encrypted_token_data
ENCRYPTION_KEY=your-encryption-key

# Autodesk/BuildingConnected API Configuration (for project monitoring)
AUTODESK_CLIENT_ID=your-autodesk-app-client-id
AUTODESK_CLIENT_SECRET=your-autodesk-app-client-secret
AUTODESK_ENCRYPTED_REFRESH_TOKEN=iv:encrypted_token_data
AUTODESK_ENCRYPTION_KEY=your-autodesk-encryption-key

# Default recipient for bid reminders
DEFAULT_EMAIL_RECIPIENT=your-default-recipient@example.com

# LangSmith Configuration (for tracing and observability)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY=your-langsmith-api-key
LANGSMITH_PROJECT=your-langsmith-project